{
  "name": "genxdb",
  "version": "2.0.0",
  "description": "GenXDB is a command-line interface tool that is used to design and generate models easily and quickly<br> using the json format based on different orms such as TypeORM and Prisma",
  "keywords": [
    "cli",
    "orm",
    "typeorm",
    "sequelize",
    "prisma",
    "mysql",
    "postgresql",
    "sqlserver"
  ],
  "main": "index.js",
  "bin": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepare": "tsc",
    "start": "npm run rm && tsc && node dist/",
    "rm": "rimraf entities/ src/entities/ prisma/"
  },
  "author": "Fiido (https://github.com/RBosio)",
  "license": "MIT",
  "homepage": "https://github.com/RBosio/genXDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/RBosio/genXDB"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.14",
    "jsonfile": "^6.1.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/jsonfile": "^6.1.4",
    "@types/shelljs": "^0.8.15",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
